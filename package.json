{
  "name": "nextjs-serverless-deploy",
  "version": "0.1.0",
  "main": "pages/index.tsx",
  "author": "Bob Engel",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "lint": "next lint",
    "format": "next lint --fix",
    "test": "jest",
    "test-update": "jest --update-snapshot",
    "test-coverage": "jest --coverage",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "prepare": "husky install",
    "cdk": "cdk",
    "deploy": "ts-node deploy/bin.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "aws-cdk": "^1.128.0",
    "@aws-cdk/aws-cloudfront-origins": "^1.120.0",
    "@aws-cdk/aws-lambda-event-sources": "^1.120.0",
    "@aws-cdk/aws-s3-deployment": "^1.120.0",
    "@aws-cdk/aws-secretsmanager": "^1.120.0",
    "@aws-cdk/aws-sns": "^1.120.0",
    "@aws-cdk/core": "^1.120.0",
    "@aws-cdk/pipelines": "^1.120.0",
    "@sls-next/cdk-construct": "^3.6.0",
    "@sls-next/lambda-at-edge": "^3.6.0",
    "@next/eslint-plugin-next": "^12.0.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/node": "17.0.5",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.0.4",
    "jest-watch-typeahead": "^0.6.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "4.5.4",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4"
  }
}
